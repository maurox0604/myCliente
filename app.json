{
  "expo": {
    "name": "IceQueenApp",
    "slug": "IceQueenApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "entryPoint": "./node_modules/expo-router/entry",

      "web": {
      "output": "static",
      "bundler": "metro"
    }

    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.companyname.IceQueenApp",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png",
      "output": "static",
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "ee673c4a-edea-4fe9-bbdd-5ecbc464447a"
      }
    },
    "plugins": [
      "expo-router",
      "expo-image-picker"
    ]
  }
}
